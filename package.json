    "devDependencies": {
        "@tsconfig/node16": "^1.0.3",
        "@types/bcrypt": "^5.0.0",
        "@types/bcryptjs": "^2.4.3",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.2",
